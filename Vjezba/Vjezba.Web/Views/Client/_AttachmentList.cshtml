@model List<Vjezba.Model.Attachement>

<div>
    <ul class="list-group mt-2">
        @foreach (var attachment in Model)
        {
            <li class="list-group-item list-group-item-light">@attachment.FilePath 
                <button type="button" onClick="deleteAttachment(@attachment.ID)" class="btn btn-danger">Izbriši</button> 
            </li>
        }
    </ul>
</div>